@model front.Models.AtualizaOutroUserModel;




<form method="post" class="form" id="atualizarForm">

        <div asp-validation-summary="All" class="text-danger mb-3"></div>

        <div class="form-group mb-1">
            <label asp-for="Email" class="control-label"></label>
        <input asp-for="Email" class="form-control" id="EmailAtualizaOutroUserModel" />
            <span asp-validation-for="Email" class="text-danger"></span>
        </div>

        <div class="form-group mb-1">
            <label asp-for="PhoneNumber" class="control-label"></label>
            <input asp-for="PhoneNumber" class="form-control" id="PhoneNumberAtualizaOutroUserModel" />
            <span asp-validation-for="PhoneNumber" class="text-danger"></span>
        </div>

        <div class="form-group mb-1">
            <label asp-for="Cpf" class="control-label"></label>
            <input asp-for="Cpf" class="form-control" id="cpfAtualizaOutroUserModel" />
            <span asp-validation-for="Cpf" class="text-danger"></span>
        </div>

        <div class="form-group mb-1">
            <label asp-for="Name" class="control-label"></label>
            <input asp-for="Name" class="form-control" id="NameAtualizaOutroUserModel" />
            <span asp-validation-for="Name" class="text-danger"></span>
        </div>

        <div class="form-group mb-1">
            <label asp-for="CodigoUnico" class="control-label"></label>
            <input asp-for="CodigoUnico" class="form-control" readonly id="CodigoUnicoAtualizaOutroUserModel" />
        </div>

        <button type="button" class="btn btn-outline-primary gap-1 d-flex align-items-center" onclick="AtualizaUsuario();">
            Atualizar Usuário
            <svg xmlns="http://www.w3.org/2000/svg" width="16" height="16" fill="currentColor" class="bi bi-person-plus-fill" viewBox="0 0 16 16">
                <path d="M1 14s-1 0-1-1 1-4 6-4 6 3 6 4-1 1-1 1zm5-6a3 3 0 1 0 0-6 3 3 0 0 0 0 6" />
                <path fill-rule="evenodd" d="M13.5 5a.5.5 0 0 1 .5.5V7h1.5a.5.5 0 0 1 0 1H14v1.5a.5.5 0 0 1-1 0V8h-1.5a.5.5 0 0 1 0-1H13V5.5a.5.5 0 0 1 .5-.5" />
            </svg>
        </button>

    </form>


    @{
        await Html.RenderPartialAsync("_ValidationScriptsPartial");
    }
